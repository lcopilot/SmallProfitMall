<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itcast.dao.AddressDao">
    <insert id="addAddress" parameterType="cn.itcast.domain.user.User">
         INSERT INTO user (uid,name,password,sex,phone,email,image,token) VALUES (#{uid},#{name},#{password},#{sex},#{phone},#{email},#{image},#{token})
    </insert>
</mapper>