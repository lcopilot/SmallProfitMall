<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itcast.dao.ShoppingCartDao">
    <!--添加购物车-->
    <insert id="addShoppingCar" parameterType="cn.itcast.domain.address.Address">
     INSERT INTO ShoppingCar
		(userId,productId,productDeploy,quantity)
	VALUES
	    (#{userId},#{productId},#{productDeploy},#{quantity})
    </insert>

</mapper>